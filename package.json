{
  "name": "backend",
  "private": true,
  "version": "0.0.1",
  "description": "Template for back-end applications.",
  "license": "UNLICENSED",
  "main": "build/src/server.js",
  "scripts": {
    "build": "tsup src --out-dir build",
    "start": "node build/server.js",
    "start:dev": "tsx watch src/server.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" ",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "test": "vitest run --dir src/application/use-cases",
    "test:watch": "vitest --dir src/application/use-cases",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest run --dir src/infra/http/controllers",
    "test:e2e:watch": "vitest --dir src/infra/http/controllers",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@fastify/jwt": "^6.7.1",
    "@prisma/client": "^4.12.0",
    "dotenv": "^16.0.3",
    "fastify": "^4.15.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@joaoalbertosv/eslint-config": "^1.10.0",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "@vitest/coverage-c8": "^0.29.8",
    "@vitest/ui": "^0.29.8",
    "eslint": "^8.37.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^4.12.0",
    "supertest": "^6.3.3",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.2",
    "vite-tsconfig-paths": "^4.0.7",
    "vitest": "^0.29.8"
  }
}
